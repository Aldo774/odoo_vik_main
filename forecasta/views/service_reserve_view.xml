<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>


		<!-- tree -->
		<record id="view_tree_service_reserve" model="ir.ui.view">
			<field name="name">service.reserve.list</field>
			<field name="model">service.reserve</field>
	        <field name="arch" type="xml">
	            <tree>
					<field name="name" invisible="1"/>
					<field name="product_id"/>
					<field name="reserve_start_date"/>
					<field name="reserve_end_date"/>
					<field name="product_qty"/>
					<field name="bom_line_service_reserve_id"/>
					<field name="bom_service_reserve_id"/>
	            </tree>
	        </field>
		</record>


		<!-- form -->
		<record id="view_form_service_reserve" model="ir.ui.view">
			<field name="name">service.reserve.form</field>
			<field name="model">service.reserve</field>
	        <field name="arch" type="xml">
	             <form string="Reserve">
	                <sheet>
	                    <group >
							<field name="name" invisible="1"/>
							<field name="product_id"/>
							<field name="reserve_start_date"/>
							<field name="reserve_end_date"/>
							<field name="product_qty"/>

							<field name="bom_line_service_reserve_id"/>
							<field name="bom_service_reserve_id"/>

						</group>

	                </sheet>
	            </form>
	        </field>
		</record>



            <act_window id="action_service_reserve"
                        name="Service Reserve"
                        res_model="service.reserve"
                        view_mode="tree,form" />


            <menuitem   id="menu_service_reserve2"
                        name="Service Reserve"
                        action="action_service_reserve"
                        parent="menu_forecasta_forecast"
							/>


	</data>
</openerp>